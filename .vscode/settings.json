{
  "workbench.colorCustomizations": {
    "tree.indentGuidesStroke": "#00ff00",
    "editor.selectionBackground": "#9e569e",
    "editor.selectionHighlightBackground": "#ff0000",
    "editor.findMatchBackground": "#00cc44a8",
    "editor.findMatchHighlightBackground": "#ff7b00a1",
    "activityBar.activeBackground": "#097f98",
    "activityBar.activeBorder": "#42769d",
    "activityBar.background": "#097f98",
    "activityBar.foreground": "#e7e7e7",
    "activityBar.inactiveForeground": "#e7e7e799",
    "activityBarBadge.background": "#f887e4",
    "activityBarBadge.foreground": "#15202b",
    "statusBar.background": "#065768",
    "statusBar.foreground": "#e7e7e7",
    "statusBarItem.hoverBackground": "#097f98",
    "titleBar.activeBackground": "#065768",
    "titleBar.activeForeground": "#e7e7e7",
    "titleBar.inactiveBackground": "#06576899",
    "titleBar.inactiveForeground": "#e7e7e799",
    "sash.hoverBorder": "#097f98",
    "statusBarItem.remoteBackground": "#065768",
    "statusBarItem.remoteForeground": "#e7e7e7",
    "commandCenter.border": "#e7e7e799"
  },
  "peacock.color": "#065768",
  "workbench.tree.indent": 15,
  "typescript.inlayHints.variableTypes.enabled": false,
  "typescript.inlayHints.propertyDeclarationTypes.enabled": false,
  "typescript.inlayHints.functionLikeReturnTypes.enabled": false,
  "typescript.inlayHints.enumMemberValues.enabled": false,
  "typescript.inlayHints.parameterNames.enabled": "all",
  "typescript.tsdk": ".yarn/sdks/typescript/lib",
  "typescript.enablePromptUseWorkspaceTsdk": true,
  "typescript.preferences.importModuleSpecifierEnding": "js",
  "javascript.preferences.importModuleSpecifierEnding": "js",
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "editor.wordWrap": "on",
  "[javascript]": {
    "editor.defaultFormatter": "rome.rome"
  },
  "[typescript]": {
    "editor.defaultFormatter": "rome.rome"
  },
  "files.trimTrailingWhitespace": true,
  "files.insertFinalNewline": true,
  "search.exclude": {
    "**/.yarn": true,
    "**/.pnp.*": true,
    "**/dist/": true,
    "**/node_modules/": true
  },
  "cSpell.words": ["anzen", "kado"],
  "docwriter.style": "JSDoc",
  "todo-tree.general.tags": [
    "TODO",
    "FIXME",
    "BUG",
    "NOBUG",
    "REQ",
    "RFE",
    "IDEA",
    "???",
    "!!!",
    "HACK",
    "PORT",
    "CAVEAT",
    "NOTE",
    "FAQ",
    "GLOSS",
    "SEE",
    "TODOC",
    "CRED",
    "STAT",
    "RVD"
  ]
}
