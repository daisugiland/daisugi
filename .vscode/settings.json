{
  "[javascript]": {
    "editor.defaultFormatter": "biomejs.biome"
  },
  "[typescript]": {
    "editor.defaultFormatter": "biomejs.biome"
  },
  "cSpell.words": [
    "anzen",
    "ayamari",
    "daisugi",
    "daisugiland",
    "eventbus",
    "kado",
    "kintsugi",
    "nekobasu",
    "urandom"
  ],
  "docwriter.style": "JSDoc",
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "editor.inlayHints.enabled": "offUnlessPressed",
  "editor.minimap.enabled": false,
  "editor.stickyScroll.enabled": true,
  "editor.wordWrap": "on",
  "files.insertFinalNewline": true,
  "files.trimTrailingWhitespace": true,
  "javascript.preferences.importModuleSpecifierEnding": "js",
  "outline.showProperties": false,
  "peacock.color": "#065768",
  "search.exclude": {
    "**/dist/": true,
    "**/node_modules/": true
  },
  "editor.codeActionsOnSave": {
    "quickfix.biome": "explicit",
    "source.organizeImports.biome": "explicit",
    "source.addMissingImports": "explicit",
    "source.fixAll.sortJSON": "explicit"
  },
  "sortJSON.orderOverride": [
    "name",
    "version",
    "description",
    "type",
    "private",
    "script",
    "license",
    "sideEffects",
    "exports",
    "scripts",
    "extends"
  ],
  "sortJSON.orderUnderride": [
    "dependencies",
    "devDependencies",
    "peerDependencies"
  ],
  "todo-tree.general.tags": [
    "!!!",
    "???",
    "BUG",
    "CAVEAT",
    "CRED",
    "FAQ",
    "FIXME",
    "GLOSS",
    "HACK",
    "IDEA",
    "NOBUG",
    "NOTE",
    "PORT",
    "REQ",
    "RFE",
    "RVD",
    "SEE",
    "STAT",
    "TODO",
    "TODOC"
  ],
  "todo-tree.highlights.customHighlight": {
    "FIXME": {
      "background": "#da1e28",
      "foreground": "#eaeaea",
      "gutterIcon": true,
      "iconColour": "#da1e28",
      "type": "line"
    },
    "NOTE": {
      "background": "#808080",
      "foreground": "#eaeaea",
      "gutterIcon": true,
      "icon": "log",
      "iconColour": "#808080"
    },
    "SEE": {
      "background": "#4ac1ec",
      "foreground": "#eaeaea",
      "gutterIcon": true,
      "icon": "eye",
      "iconColour": "#4ac1ec"
    },
    "TODO": {
      "background": "#CEA703",
      "foreground": "#eaeaea",
      "gutterIcon": true,
      "icon": "checklist",
      "iconColour": "#CEA703"
    }
  },
  "todo-tree.regex.regex": "(//|#|<!--|/\\*|^\\s*\\*)\\s*($TAGS)",
  "typescript.tsdk": "./node_modules/typescript/lib",
  "typescript.enablePromptUseWorkspaceTsdk": true,
  "typescript.inlayHints.enumMemberValues.enabled": false,
  "typescript.inlayHints.functionLikeReturnTypes.enabled": false,
  "typescript.inlayHints.parameterNames.enabled": "all",
  "typescript.inlayHints.propertyDeclarationTypes.enabled": false,
  "typescript.inlayHints.variableTypes.enabled": false,
  "typescript.preferences.importModuleSpecifierEnding": "js",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "workbench.colorCustomizations": {
    "activityBar.activeBackground": "#097f98",
    "activityBar.activeBorder": "#42769d",
    "activityBar.background": "#097f98",
    "activityBar.foreground": "#e7e7e7",
    "activityBar.inactiveForeground": "#e7e7e799",
    "activityBarBadge.background": "#f887e4",
    "activityBarBadge.foreground": "#15202b",
    "commandCenter.border": "#e7e7e799",
    "editor.findMatchBackground": "#00cc44a8",
    "editor.findMatchHighlightBackground": "#ff7b00a1",
    "editor.selectionBackground": "#9e569e",
    "editor.selectionHighlightBackground": "#ff0000",
    "sash.hoverBorder": "#097f98",
    "statusBar.background": "#065768",
    "statusBar.foreground": "#e7e7e7",
    "statusBarItem.hoverBackground": "#097f98",
    "statusBarItem.remoteBackground": "#065768",
    "statusBarItem.remoteForeground": "#e7e7e7",
    "titleBar.activeBackground": "#065768",
    "titleBar.activeForeground": "#e7e7e7",
    "titleBar.inactiveBackground": "#06576899",
    "titleBar.inactiveForeground": "#e7e7e799",
    "tree.indentGuidesStroke": "#00ff00"
  },
  "workbench.tree.indent": 15
}
